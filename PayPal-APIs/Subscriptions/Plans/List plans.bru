meta {
  name: List plans
  type: http
  seq: 2
}

get {
  url: {{base_url}}/v1/billing/plans?page_size=10&page=1&total_required=true
  body: none
  auth: inherit
}

params:query {
  page_size: 10
  page: 1
  total_required: true
  ~product_id: {{product_id}}
  ~plan_ids: {{billing_plan_id}}
}

headers {
  Prefer: {{prefer_representation_detailed}}
  ~Content-Type: application/json
  ~PayPal-Request-Id: {{$guid}}
  ~PayPal-Client-Metadata-Id: {{paypal_client_metadata_Id}}
  ~PayPal-Partner-Attribution-Id: {{paypal_partner_attribution_id}}
  ~PayPal-Auth-Assertion: {{paypal_auth_assertion}}
  ~Prefer: {{prefer_representation_minimal}}
}

settings {
  encodeUrl: true
}

docs {
  Lists billing plans.
}
